  "description": "",
  "main": "./providers/AdonisTurnstileProvider.ts",
  "scripts": {
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc && npm run copy:files",
    "build": "npm run clean && npm run compile",
    "prerelease": "npm run clean && tsc",
    "copy:files": "copyfiles \"templates/**/*.txt\" build",
    "test": "node --loader=ts-node/esm bin/test.ts",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "changeset": "changeset",
    "husky": "husky install .husky"
  },
  "keywords": [
    "adonis",
    "adonisjs",
    "adonis-framework",
    "turnstile",
    "adonis-turnstile"
  ],
  "homepage": "https://github.com/tuanvu0995/adonis-turnstile",
  "url": "https://github.com/tuanvu0995/adonis-turnstile",
  "email": "tuanvu0995@gmail.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuanvu0995/adonis-turnstile.git"
  },
  "author": "Vu Lai <tuanvu0995@gmail.com>",
  "bugs": {
    "url": "https://github.com/tuanvu0995/adonis-turnstile/issues"
  },
  "license": "MIT",
  "types": "./build/adonis-typings/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.13",
    "@adonisjs/sink": "^5.4.3",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@commitlint/config-conventional": "^17.6.5",
    "@japa/assert": "^1.4.1",
    "@japa/run-failed-tests": "^1.1.1",
    "@japa/runner": "^2.5.1",
    "@japa/spec-reporter": "^1.3.3",
    "@types/node": "^20.2.5",
    "@types/node-fetch": "^2.6.4",
    "chance": "^1.1.11",
    "change": "^0.0.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "japa": "^4.0.0",
    "nock": "^13.3.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "files": [
    "build/adonis-typings",
    "build/Providers",
    "build/templates",
    "build/instructions.js",
    "build/src",
    "build/package.json"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "adonisjs": {
    "instructions": "./build/instructions.js",
    "templates": {
      "config": {
        "src": "config/turnstile.txt",
        "dest": "turnstile.ts"
      }
    },
    "env": {
      "TURNSTILE_SITE_KEY": "",
      "TURNSTILE_SECRET_KEY": ""
    },
    "types": "adonis-turnstile",
    "providers": [
      "adonis-turnstile"
    ]
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
